{
	"version": "1.0.1",
	"name": "@use-pico/env",
	"description": "Next.js env helper stuff.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/viv.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
	"type": "module",
	"main": "lib/index.js",
