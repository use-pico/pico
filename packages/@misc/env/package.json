		"@use-pico/tsconfig": "^1.0.1",
		"@types/node": "^20.4.2",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/leight-core/viv/issues"
	},
	"homepage": "https://github.com/leight-core/viv#readme"
