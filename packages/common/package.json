{
	"name": "@use-pico/common",
	"description": "Shared stuff between client/server.",
	"version": "5.0.100",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"typecheck": "tsc --noEmit",
		"no:build": "bun x rollup -c",
		"test": "vitest --run"
	},
	"dependencies": {
		"@apollo/client": "4.0.9",
		"@graphql-typed-document-node/core": "3.2.0",
		"@paralleldrive/cuid2": "3.1.0",
		"@types/is-callable": "1.1.2",
		"@types/is-empty": "1.2.3",
		"@types/luxon": "3.7.1",
		"@types/qs": "6.14.0",
		"fast-clean": "1.5.2",
		"flattie": "1.1.1",
		"is-callable": "1.2.7",
		"is-empty": "1.2.0",
		"is-string": "1.1.1",
		"js-sha256": "0.11.1",
		"kysely": "0.28.8",
		"luxon": "3.7.2",
		"preferred-locale": "2.0.2",
		"qs": "6.14.0",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "29.0.0",
		"@rollup/plugin-node-resolve": "16.0.3",
		"@rollup/plugin-typescript": "12.3.0",
		"@types/node": "24.10.0",
		"@types/react": "19.2.2",
		"@use-pico/tsconfig": "5.0.0",
		"rollup": "4.52.5",
		"rollup-plugin-dts": "6.2.3",
		"typescript": "5.9.3",
		"vitest": "4.0.7"
	},
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"files": [
		"src"
	],
	"exports": {
		"./*": {
			"types": "./src/*/index.ts",
			"import": "./src/*/index.ts"
		}
	},
	"license": "MIT",
	"repository": "use-pico/pico",
	"homepage": "https://github.com/use-pico/pico#readme",
	"bugs": "https://github.com/use-pico/pico/issues"
}
