{
	"version": "2.0.1",
	"name": "@use-pico/ui-extra",
	"description": "Components with quite heavy dependencies on the other @pico packages.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/use-pico2/pico"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@mantine/core": "7.1.5",
		"@mantine/modals": "7.1.5",
		"@mantine/notifications": "7.1.5",
		"@tabler/icons-react": "^2.39.0",
		"@use-pico/file": "^2.0.0",
		"@use-pico/i18n": "^2.0.1",
		"@use-pico/navigation": "^2.0.1",
		"@use-pico/query": "^2.0.0",
		"@use-pico/schema": "^2.0.0",
		"@use-pico/store": "^2.0.0",
		"@use-pico/ui": "^2.0.1",
		"@use-pico/utils": "^2.0.0",
		"axios": "^1.5.1",
		"js-file-downloader": "^1.1.25",
		"next": "^14.0.1",
		"next-auth": "^4.24.4",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^2.0.0",
		"@use-pico/eslint-config-eslint": "^2.0.0",
		"@use-pico/tsconfig": "^2.0.0",
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.15",
		"@types/uuid": "^9.0.2",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/use-pico2/pico/issues"
	},
	"homepage": "https://github.com/use-pico2/pico#readme"
}
