{
	"version": "1.0.0",
	"name": "@use-pico/utils",
	"description": "Some simple utils used in @pico libraries.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/viv.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.1",
		"@use-pico/types": "^1.0.0",
		"@types/classnames": "^2.3.1",
		"@types/humanize-duration": "^3.27.1",
		"@types/is-callable": "^1.1.0",
		"@types/is-empty": "^1.2.1",
		"@types/is-object": "^1.0.2",
		"@types/is-string": "^1.0.0",
		"build-url-ts": "^6.1.5",
		"classnames": "^2.3.2",
		"clsx": "^2.0.0",
		"deepmerge": "^4.3.1",
		"devalue": "^4.3.2",
		"fast-clean": "^1.3.1",
		"filesize": "^10.0.12",
		"humanize-duration": "^3.29.0",
		"is-callable": "^1.2.7",
		"is-empty": "^1.2.0",
		"is-object": "^1.0.2",
		"is-string": "^1.0.7",
		"path-to-regexp": "^6.2.1",
		"react": "^18.2.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^1.0.0",
		"@use-pico/eslint-config-eslint": "^1.0.0",
		"@use-pico/tsconfig": "^1.0.0",
		"@types/node": "^20.5.3",
		"@types/react": "^18.2.21",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.47.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/leight-core/viv/issues"
	},
	"homepage": "https://github.com/leight-core/viv#readme"
}
