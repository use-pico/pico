{
	"version": "1.0.6",
	"name": "@use-pico/sdk",
	"description": "SDK generator.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/viv.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@use-pico/generator": "^1.0.6",
		"@use-pico/types": "^1.0.6",
		"@use-pico/utils": "^1.0.20",
		"chalk": "^5.3.0",
		"eta": "^3.1.0",
		"fast-glob": "^3.3.1"
	},
	"devDependencies": {
		"@use-pico/esbuild": "^1.0.4",
		"@use-pico/eslint-config-eslint": "^1.0.4",
		"@use-pico/tsconfig": "^1.0.6",
		"@types/node": "^20.5.3",
		"concurrently": "^8.2.1",
		"del-cli": "^5.0.0",
		"eslint": "^8.47.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/leight-core/viv/issues"
	},
	"homepage": "https://github.com/leight-core/viv#readme"
}
