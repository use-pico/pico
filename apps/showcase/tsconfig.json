		/* Base Options: */
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"declaration": false,
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		/* Strictness */
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"checkJs": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"module": "ESNext",
		"noEmit": true,
		"moduleResolution": "Bundler",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"types": [
			"vite/client"
		],
		"jsx": "preserve",
		"jsxImportSource": "react",
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"~/*": [
				"./src/*"
			]
		}
	},
